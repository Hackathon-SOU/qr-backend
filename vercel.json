{
    "version": 2,
    "name": "qr-backend",
    "builds": [
      { "src": "index.js", "use": "@vercel/node" },
      {
        "src": "public/**",
        "use": "@vercel/static"
    }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.js" },
      {
        "src": "/swagger-ui.css",
        "dest": "./public/swagger-ui.css"
    },{
      "src":"/bhanu",
      "dest":"./public/index.html"
    }
    ]
  }