{
  "version": 2,
  "name": "qr-backend",
  "builds": [{
      "src": "src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "uploads/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [{
    "src": "/(.*)",
    "dest": "src/index.js"
  }]
}